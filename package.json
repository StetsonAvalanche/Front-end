{
  "name": "gittalk",
  "version": "1.0.0",
  "description": "slack for github users",
  "main": "server/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm start && npm run test-spec",
    "test-spec": "mocha test/helpers/browser.js test/client/*.spec.js test/server/*.spec.js",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./",
    "start-dev": "webpack -w | nodemon server/server.js",
    "start": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StetsonAvalanche/GitTalk.git"
  },
  "keywords": [
    "slack",
    "github",
    "chat",
    "communication"
  ],
  "author": "Afsoon Nickram, Chase Starr, Felicia Fong, Tony Tan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StetsonAvalanche/GitTalk/issues"
  },
  "homepage": "https://github.com/StetsonAvalanche/GitTalk#readme",
  "dependencies": {
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "zombie": "^5.0.4"
  }
}
